{% extends 'pagelayout.html.twig' %}

{% block content %}
    <div class="bgded overlay"
         {% if not ez_is_field_empty(content, 'image_background') %}
         style="background-image:url('{{ ez_field_value(content, 'image_background').uri }}');"
         {% endif %}
    >
        <div id="pageintro" class="hoc clear">
            <article>
                {% if not ez_is_field_empty(content, 'title') %}
                    <h3 class="heading">{{ ez_field_value(content, 'title') }}</h3>
                {% endif %}

                {% if not ez_is_field_empty(content, 'description') %}
                    {{ ez_render_field(content, 'description') }}
                {% endif %}
            </article>
        </div>
    </div>
    <div class="wrapper row3">
        <main class="hoc container clear">
            <section id="services">
                {% if not ez_is_field_empty(content, 'title_relation') %}
                    <h6 class="heading font-x3 center">{{ ez_field_value(content, 'title_relation') }}</h6>
                {% endif %}

                {% if not ez_is_field_empty(content, 'description_forward_kitchen') %}
                    {{ ez_render_field(content, 'description_forward_kitchen') }}
                {% endif %}

                {% if kitchens is not null %}
                    <ul class="nospace group">
                        {% for kitchen in kitchens %}
                            {{ dump(kitchen.contentInfo.mainLocationId) }}
                            {{ dump(path(kitchen)) }}
                            <li class="one_quarter">
                                <article class="height-tile"
                                    {% if not ez_is_field_empty(kitchen, 'image_tile') %}
                                        style="background-image:url('{{ ez_field_value(kitchen, 'image_tile').uri }}');"
                                    {% endif %}
                                >
                                    <h6 class="heading bg-white">{{ ez_field_value(kitchen, 'title') }}</h6>
                                </article>
                            </li>
                        {% endfor %}
                    </ul>
                {% endif %}
            </section>
            <!-- ################################################################################################ -->
            <!-- / main body -->
            <div class="clear"></div>
        </main>
    </div>
